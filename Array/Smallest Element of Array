// Smallest value in array
package Array;

import java.util.Scanner;

public class P2 {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
        int arr[]=new int [n];
        int min=1000;
        for (int i = 0; i < n; i++) {
            arr[i]=sc.nextInt();
            if(arr[i]<min){
                min=arr[i];
            }
        }
        System.out.println(min);
    }
}
